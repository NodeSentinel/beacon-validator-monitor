{
  "name": "telegram-bot",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Telegram bot starter template",
  "imports": {
    "@/*": "./build/*"
  },
  "author": "Nicolás Domínguez <nf.dominguez.87@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"tsx ./src/main.ts\"",
    "start": "tsc && tsx ./src/main.ts",
    "start:force": "tsx ./src/main.ts",
    "test": "echo \"No tests for telegram-bot\""
  },
  "dependencies": {
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/commands": "1.0.8",
    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.1.2",
    "@grammyjs/parse-mode": "1.11.1",
    "@grammyjs/runner": "2.0.3",
    "@grammyjs/types": "3.20.0",
    "@hono/node-server": "1.14.2",
    "@t3-oss/env-core": "^0.10.1",
    "callback-data": "1.1.1",
    "dotenv": "^16.0.3",
    "grammy": "1.36.1",
    "hono": "4.10.3",
    "iso-639-1": "3.1.5",
    "pino": "9.9.0",
    "pino-pretty": "13.0.0",
    "tsx": "4.20.4",
    "valibot": "0.42.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "tsc-watch": "^6.3.1",
    "typescript": "^5.9.2"
  }
}
